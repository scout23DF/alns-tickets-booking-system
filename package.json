{
    "name": "alns-tickets-booking-system",
    "version": "0.0.0",
    "private": true,
    "description": "ALNS-Tickets-Booking-System",
    "license": "UNLICENSED",
    "type": "module",
    "scripts": {
        "build": "npm-run-all build:*",
        "build:tsc": "tsc -b",
        "build:vite": "vite build --emptyOutDir",
        "dev": "npm-run-all --parallel dev:*",
        "dev:vite": "vite",
        "e2e": "playwright test --ui --config src/test/webapp/e2e/playwright.config.ts",
        "e2e:headless": "playwright test --config src/test/webapp/e2e/playwright.config.ts",
        "lint": "eslint .",
        "prepare": "husky",
        "prettier:check": "prettier --check .",
        "prettier:format": "prettier --write .",
        "preview": "vite preview",
        "start": "vite",
        "test": "npm run watch:test",
        "test:component": "start-server-and-test dev http://localhost:9000 'playwright test --ui --config src/test/webapp/component/playwright.config.ts'",
        "test:component:headless": "start-server-and-test dev http://localhost:9000 'playwright test --config src/test/webapp/component/playwright.config.ts'",
        "test:coverage": "vitest run --coverage",
        "watch": "npm-run-all --parallel watch:*",
        "watch:test": "vitest --",
        "watch:tsc": "tsc --noEmit --watch"
    },
    "dependencies": {
        "i18next": "25.2.0",
        "i18next-browser-languagedetector": "8.1.0",
        "i18next-http-backend": "3.0.2",
        "piqure": "2.1.1",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-i18next": "15.5.2"
    },
    "devDependencies": {
        "@eslint/js": "9.27.0",
        "@playwright/test": "1.52.0",
        "@prettier/plugin-xml": "3.4.1",
        "@testing-library/dom": "10.4.0",
        "@testing-library/react": "16.3.0",
        "@tsconfig/vite-react": "3.4.0",
        "@types/node": "22.15.21",
        "@types/react": "19.1.5",
        "@types/react-dom": "19.1.5",
        "@typescript-eslint/eslint-plugin": "8.32.1",
        "@typescript-eslint/parser": "8.32.1",
        "@vitejs/plugin-react": "4.5.0",
        "@vitest/coverage-istanbul": "3.1.4",
        "eslint": "9.27.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-react": "7.37.5",
        "globals": "16.1.0",
        "husky": "9.1.7",
        "jsdom": "26.1.0",
        "lint-staged": "16.0.0",
        "npm-run-all2": "8.0.3",
        "prettier": "3.5.3",
        "prettier-plugin-gherkin": "3.1.2",
        "prettier-plugin-java": "2.6.7",
        "prettier-plugin-organize-imports": "4.1.0",
        "prettier-plugin-packagejson": "2.5.14",
        "start-server-and-test": "2.0.12",
        "typescript": "5.8.3",
        "typescript-eslint": "8.32.1",
        "vite": "6.3.5",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.1.4",
        "vitest-sonar-reporter": "2.0.1"
    },
    "engines": {
        "node": ">=22"
    }
}
